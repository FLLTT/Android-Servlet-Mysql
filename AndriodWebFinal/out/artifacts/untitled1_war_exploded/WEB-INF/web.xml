<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="false">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>UserListServlet</servlet-name>
        <servlet-class>UserListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserListServlet</servlet-name>
        <url-pattern>/userlist.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UsernameViewServlet</servlet-name>
        <servlet-class>UsernameViewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsernameViewServlet</servlet-name>
        <url-pattern>/usernameview.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserRegisterServlet</servlet-name>
        <servlet-class>UserRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserRegisterServlet </servlet-name>
        <url-pattern>/save.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PasswordExServlet</servlet-name>
        <servlet-class>PasswordExServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PasswordExServlet</servlet-name>
        <url-pattern>/change.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DietListServlet</servlet-name>
        <servlet-class>DietListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DietListServlet</servlet-name>
        <url-pattern>/dietlist.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OrderInfoSaveServlet</servlet-name>
        <servlet-class>OrderInfoSaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderInfoSaveServlet</servlet-name>
        <url-pattern>/ordersave.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OrderInfoReadServlet</servlet-name>
        <servlet-class>OrderInfoReadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderInfoReadServlet</servlet-name>
        <url-pattern>/orderread.do</url-pattern>
    </servlet-mapping>
</web-app>
